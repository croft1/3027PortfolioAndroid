<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="croft.todo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/tick"
        android:label="Carter 3027 A1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ReminderApplication.ViewListActivity" />
        <activity android:name=".ReminderApplication.addReminderActivity" />
        <activity android:name=".NameGenerator.NameGenMain" />
        <activity android:name=".NameGenerator.NameViewActivity" />
        <activity android:name=".Calculator.CalculatorMain" />
        <activity android:name="croft.todo.MonsterMain" />
        <activity android:name="croft.todo.AddMonsterActivity" />
        <activity android:name=".ReminderApplication.ViewDetailedReminderActivity"></activity>
    </application>

</manifest>